{
  "extends": ["next", "next/core-web-vitals"],
  "rules": {
    "import/order": [
      "warn",
      {
        "groups": [["builtin", "external"], ["internal", "parent"], ["sibling", "index"], "type", "object"],
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
